{
  "name": "doli-theme",
  "publisher": "lqyld",
  "displayName": "Doli Theme",
  "description": "Doli Theme for VS Code",
  "version": "0.1.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/LQYld/vscode-theme.git"
  },
  "bugs": {
    "url": "https://github.com/LQYld/vscode-theme.git/issues"
  },
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "theme",
    "Doli",
    "doli",
    "dark"
  ],
  "icon": "icon.png",
  "contributes": {
    "themes": [
      {
        "label": "Doli Universal",
        "uiTheme": "vs-dark",
        "path": "./themes/Doli-universal.json"
      },
      {
        "label": "Doli Visual Studio",
        "uiTheme": "vs-dark",
        "path": "./themes/Doli-visual-studio.json"
      },
      {
        "label": "Doli Soft Green",
        "uiTheme": "vs",
        "path": "./themes/Doli-soft-green.json"
      }
    ]
  },
  "scripts": {
    "install vsce": "pnpm install vsce -g --save vsce",
    "build": "vsce package",
    "publish": "vsce publish"
  }
}
